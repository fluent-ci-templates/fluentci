{
  "importMap": "import_map.json",
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "esm:add": "deno run -A https://esm.sh/v128 add",
    "esm:update": "deno run -A https://esm.sh/v128 update",
    "esm:remove": "deno run -A https://esm.sh/v128 remove"
  },
  "fmt": {
    "exclude": [
      ".fluentci/",
      "src/prelude.ts"
    ]
  },
  "lint": {
    "exclude": [
      ".fluentci/",
      "src/prelude.ts"
    ]
  },
  "test": {
    "exclude": [
      ".fluentci/"
    ]
  }
}